{
    "root": true,
    "parser": "babel-eslint",
    "plugins": ["jest"],
    "extends": "adpyke-es6",
    "env": {
        "jest/globals": true
    },
    "rules": {
        "no-bitwise": "off",
        "no-lonely-if": "off",
        "class-methods-use-this": "off",
        "arrow-body-style": "off",
        "no-loop-func": "off"
    },
    "settings": {
        "react": {
        "version": "latest"
        }
    }
}